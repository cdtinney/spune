    "watch": "nodemon app.js"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^6.2.0",
    "express": "^4.18.2",
    "express-session": "^1.15.6",
    "mongoose": "^5.7.5",
    "mongoose-findorcreate": "^3.0.0",
    "passport": "^0.4.0",
    "passport-oauth2-refresh": "^1.1.0",
    "passport-spotify": "^1.0.1",
    "spotify-web-api-node": "^4.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "jest": "^23.6.0",
    "jest-puppeteer": "^3.9.0",
    "nodemon": "^1.18.10",
    "puppeteer": "^1.13.0",
    "supertest": "^3.4.2"
