    "spotify-web-api-node": "3.1.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "kill-port": "^1.3.2",
    "nodemon": "^1.18.10",
    "redux-devtools-extension": "2.13.5"
  }
}
