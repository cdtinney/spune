{
  "name": "spune",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "client": "cd client && npm run start",
    "client:build": "cd client && npm run build",
    "client:coveralls": "npm run client:test:coverage && cat client/coverage/lcov.info | coveralls ",
    "client:test": "cd client && npm run test",
    "client:test:coverage": "cd client && npm run test:coverage",
    "dev": "concurrently --kill-others \"npm run server:watch\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "install:all": "npm install && cd client && npm install",
    "server": "node server/server.js",
    "server:test": "jest --watch",
    "server:watch": "nodemon server/server.js",
    "start": "npm run server",
    "test": "npm run client:test:coverage"
  },
  "keywords": [],
  "author": "Colin Tinney",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "mongoose": "^5.4.13",
    "mongoose-findorcreate": "^3.0.0",
    "passport": "^0.4.0",
    "passport-oauth2-refresh": "^1.1.0",
    "passport-spotify": "^1.0.1",
    "react-full-screen": "^0.2.3",
    "redux": "^4.0.1",
    "reselect": "^4.0.0",
    "spotify-web-api-node": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "coveralls": "^3.0.3",
    "jest": "^23.6.0",
    "kill-port": "^1.3.2",
    "nodemon": "^1.18.10",
    "react-router-dom": "^4.3.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "supertest": "^3.4.2"
  }
}
